<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.helpMe.controller.JobDetail" xmlns:m="sap.m">
    <m:Page showHeader="true" title="Job Detail" showFooter="true" showNavButton="true" navButtonPress="_onPageNavButtonPress1">
        <m:content>
            <m:ObjectHeader intro="{company_name}" title="{position_name}" icon="" number="1,000" numberUnit="USD" numberState="None" showMarkers="false" markFavorite="false" markFlagged="false" backgroundDesign="Translucent" condensed="false">
                <m:attributes/>
                <m:statuses/>
            </m:ObjectHeader>
        </m:content>
        <m:footer/>
        <m:headerContent>
            <m:Image src="/webapp/resources/logo.png" height="auto" width="75px" densityAware="false" press="_onImagePress7"/>
        </m:headerContent>
        <m:subHeader/>
        <m:customHeader/>
    </m:Page>
</mvc:View>
