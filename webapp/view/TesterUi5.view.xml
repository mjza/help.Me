                <m:contentMiddle>
                    <core:Icon src="sap-icon://source-code" size="48px" height="48px" width="48px" visible="true"/>
                    <m:Title text="SAPUI5" titleStyle="H1" width="auto" textAlign="Begin" visible="true"/>
                </m:contentMiddle>
                <m:contentRight>
                    <m:Title text="Ongoing" titleStyle="H1" width="auto" textAlign="Begin" visible="true"/>
