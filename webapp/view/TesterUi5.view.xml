<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.helpMe.controller.TesterUi5" xmlns:m="sap.m" xmlns:core="sap.ui.core">
    <m:Page showHeader="true" title="Test Platform" showFooter="true" showNavButton="true" navButtonPress="_onPageNavButtonPress1">
